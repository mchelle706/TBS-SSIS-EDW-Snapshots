                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT date
	,SUM(ar_balance) AS ar_balance
	,SUM(unapplied_payments) AS unapplied_payments
	,SUM(invoices_outstanding) AS invoices_outstanding
	,SUM(fee_escrowed) AS fee_escrow
	,SUM(reserves_escrowed) AS reserve_escrowed
	,SUM(advanced) AS funds_advanced
	,SUM(cash_reserves) AS cash_reserves
	,SUM(total_reserves) AS total_reserves
	,SUM(loan_amount) AS loan_amount
                ,business_unit
FROM AnalyticsSandbox.dbo.fiu_client
WHERE cast(date as date) = cast(getdate() as date)
GROUP BY business_unit, date
ORDER BY business_unit, date;</property>
