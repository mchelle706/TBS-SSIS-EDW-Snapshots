                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/* NEW FIU QUERY */

With  ABC AS
(
	select InvoiceKey, Sum(AMT) as PMT_AMT 
	From CadenceReplica.dbo.Payments 
	Where CbTypeCode in(30,31,32,33)
	Group by InvoiceKey

)
,cte_ns_reserves as 
(
	select
		c.companyname
		,c.entitynumber
		,c.id
		,c.externalid
		,'TBSCAPFUND' business_unit
		,coalesce(round(sum(case when a.acctnumber = '21310' then tal.netamount else 0 end),2),0.00) cash_reserve_total
		,coalesce(round(sum(case when a.acctnumber = '21320' then tal.netamount else 0 end),2),0.00) reserve_escrow_total

	from EDWIntegration.dbo.ns_transaction_accounting_line tal
	left join EDWIntegration.dbo.ns_account a  on tal.account = a.id
	left join EDWIntegration.dbo.ns_transaction_line tl on tal.[transaction] = tl.[transaction] and tal.transactionline = tl.linesequencenumber
	left join EDWIntegration.dbo.ns_client_2 c on tl.entity = c.id
	left join (select custbody_client, sum(foreignamountunpaid) ar_balance from EDWIntegration.dbo.ns_invoices group by custbody_client) i on c.id = i.custbody_client
	
	where a.acctnumber in ('21310','21320')
	and c.custentity_tbs_entity_location = 2

	group by
	c.companyname
	,c.entitynumber
	,c.id
	,c.externalid
)
Select 
	CURRENT_TIMESTAMP Date
	,C.ClientKey
	,C.ClientNo
	,C.Name
	,cc.GrossCollateral  AR_Balance
	,coalesce (Sum(p.PMT_AMT),0) * -1 AS Unapplied_Payments 
	,CC.GrossCollateral + (coalesce (Sum(p.PMT_AMT),0)) AS Invoices_Outstanding	
	,cc.FeeEscrow As Fee_Escrowed
	,cc.RsvEscrow AS Reserves_Escrowed
	,(cc.StdAdvAmt0 +cc.StdAdvAmt1 + cc.StdAdvAmt2 + cc.StdAdvAmt3 + cc.StdAdvAmt4 + cc.StdAdvAmt5 +  (coalesce (Sum(p.PMT_AMT),0))) As Advanced
	,C.CurrRsvAmt as Cash_Reserves 
	,(cc.RsvEscrow + c.CurrRsvAmt) As Total_Reserves
	,cc.Balance AS Loan_Amount
	,C.Office 

From CadenceReplica.dbo.clients C(nolock) 
	Left join CadenceReplica.dbo.Invoices I (nolock) on c.ClientKey = i.ClientKey
	left join CadenceReplica.dbo.CalcClient CC (nolock) on c.ClientKey = cc.ClientKey
	LEFT join ABC P (nolock) on i.InvoiceKey = P.InvoiceKey
	--ADDED the following:
	INNER JOIN dbo.client ON c.ClientKey = client.row_source_system_key
	AND client.row_source_system = 'Cadence'
	INNER JOIN dbo.entity ent ON client.entity_id = ent.id

Where C.Inactive = 0 
	and CC.Balance !=0  
	and C.office = 'TBSFACTORING'
Group by 
	 C.ClientKey
	,C.ClientNo
	,C.Name
	,CC.GrossCollateral
	,cc.RecourseOver
	,C.CurrBalAmt
	,cc.FeeEscrow 
	,cc.RsvEscrow
	,cc.StdAdvAmt0
	,cc.StdAdvAmt1 
	,cc.StdAdvAmt2 
	,cc.StdAdvAmt3 
	,cc.StdAdvAmt4
	,cc.StdAdvAmt5
	,C.CurrRsvAmt
	,cc.balance
	,C.Office

union

select 
	CURRENT_TIMESTAMP date
	,c.ClientKey
	,c.clientno
	,c.name
	,coalesce(i.ar_balance,0.00) ar_balance
	,0 unapplied_payments
	,0 invoices_outstanding
	,0 fee_escrowed
	,-1*r.reserve_escrow_total reserves_escrowed
	,coalesce(i.ar_balance,0) - (-1*r.reserve_escrow_total) advanced
	,-1*r.cash_reserve_total cash_reserves
	,-1*(r.reserve_escrow_total + r.cash_reserve_total) total_reserves
	,0 loan_amount
	,r.business_unit
from 
	cte_ns_reserves r
	left join (select custbody_client, sum(foreignamountunpaid) ar_balance from EDWIntegration.dbo.ns_invoices group by custbody_client) i on r.id = i.custbody_client
	left join CadenceReplica.dbo.CLients c on r.externalid = c.ClientNo;</property>
