                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 	 date
        ,row_created_date
		,debtor_id
		,debtor_name
		,debtor_number
		,client_id
		,client_name
		,client_number
		,business_unit
		,type
		,SUM(invoice_amount) AS total_amount
		,SUM(invoice_balance) AS total_balance
		,COALESCE (SUM(CASE WHEN ar_bucket LIKE '&lt;=30' THEN  invoice_balance END), 0) AS bucket_30
		,COALESCE(SUM(CASE WHEN ar_bucket LIKE '31-60' THEN  invoice_balance END), 0) AS bucket_31_60
		,COALESCE(SUM(CASE WHEN ar_bucket LIKE '61-90' THEN  invoice_balance END), 0) AS bucket_61_90
		,COALESCE(SUM(CASE WHEN ar_bucket LIKE '91-120' THEN invoice_balance END), 0) AS bucket_91_120
		,COALESCE(SUM(CASE WHEN ar_bucket LIKE '121+' THEN   invoice_balance END), 0) AS bucket_120_up
		
  FROM dbo.ar_snapshot_raw
  
  WHERE business_unit = 'TBSFACTORING'
  and invoice_amount &lt;&gt; 0
  and invoice_balance &lt;&gt; 0
  and cast(date as date) = cast(getdate() as date)
  
  GROUP BY date
		,debtor_id
		,client_id
		,debtor_number
		,client_number
		,type
		,business_unit
		,debtor_name
		,client_name
        ,row_created_date

	ORDER BY date, type, business_unit, debtor_name, debtor_number</property>
